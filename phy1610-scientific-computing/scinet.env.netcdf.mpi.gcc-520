# setup for parallel netcdf (netcdf+mpi) using gcc-5.3, boost, rarray,
# and pgplot

module purge
module load Xlibraries extras git

module load gcc/4.6.1 openmpi/1.4.4-gcc-v4.6.1 pgplot/5.2.2-gcc

module load hdf5/187-v18-openmpi-gcc
module load netcdf/4.2.0_hdf5_openmpi-gcc

module swap gcc/5.2.0
module load boost/1.60.0-gcc5.2.0
module load gdb/7.6
module load rarray/1.1

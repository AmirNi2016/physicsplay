# setup for serial netcdf using gcc-5.3, boost, rarray, and pgplot

module purge
module load Xlibraries extras git

module load gcc/4.8.1
module load openmpi/gcc/1.6.4
module load hdf5/1811-v18-serial-gcc
module load netcdf/4.2.1.1_serial-gcc

module swap gcc/5.2.0
module load boost/1.60.0-gcc5.2.0
module load gdb/7.6
module load pgplot/5.2.2-gcc
module load rarray/1.1

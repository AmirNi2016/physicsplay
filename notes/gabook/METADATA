my %monthsOfYear = (
'January' => 1,
'February' => 2,
'March' => 3,
'April' => 4,
'May' => 5,
'June' => 6,
'July' => 7,
'August' => 8,
'September' => 9,
'October' => 10,
'November' => 11,
'December' => 12
) ;

dumpem() ;
#testm() ;

exit ;

sub dumpem
{
   #my @s = ( $metadata[0], $metadata[1], $metadata[2] ) ;
   my @sorted = sort mysort @metadata ;

   foreach (@sorted)
   {
      my ($source, $title, $date, $ref) 
         = ( $_->{'SOURCE'},
             $_->{'TITLE'},
             $_->{'DATE'},
             $_->{'REF'} ) ;

      print "$source\n$title\n$date\n$ref\n\n" ;
   }
}

sub testm
{
print $monthsOfYear{'July'} ;
die;
}

sub mysort
{
   my ($asource, $atitle, $adate, $aref) 
         = ( $$a{'SOURCE'},
             $$a{'TITLE'},
             $$a{'DATE'},
             $$a{'REF'} ) ;

   my ($bsource, $btitle, $bdate, $bref) 
         = ( $$b{'SOURCE'},
             $$b{'TITLE'},
             $$b{'DATE'},
             $$b{'REF'} ) ;

   die "date parse: $adate\n" unless ( $adate =~ /(\S+)\s+(\d+)\s*,\s*(\d+)\s*/ ) ;
   my ($amonth, $aday, $ayear) = ($1, $2, $3) ;

   die "date parse: $bdate\n" unless ( $bdate =~ /(\S+)\s+(\d+)\s*,\s*(\d+)\s*/ ) ;
   my ($bmonth, $bday, $byear) = ($1, $2, $3) ;

   my $rc = $ayear <=> $byear ;
   if ( !$rc )
   {
      $rc = $monthsOfYear{$amonth} <=> $monthsOfYear{$bmonth} ;

      if ( !$rc )
      {
         $rc = $aday <=> $bday ;
      }
   }

   return $rc ;
}

n=7
~/bin/ct gradQuantumProblemSet$n gradQuantumProblemSetN #tp$n
perl -p -i -e "s/{N}/$n/g" gradQuantumProblemSet$n.tex

for i in 1 2 3 ; do 
   cp ../gradQuantumProblemSetNProblemM.tex ../phy1520/gradQuantumProblemSet${n}Problem$i.tex
   perl -p -i -e "s/{N}/$n/g;s/{M}/$i/g;" ../phy1520/gradQuantumProblemSet${n}Problem$i.tex
   git add ../phy1520/gradQuantumProblemSet${n}Problem$i.tex
done

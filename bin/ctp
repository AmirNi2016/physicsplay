n=5
~/bin/ct gradQuantumProblemSet$n gradQuantumProblemSetN tp$n
perl -p -i -e "s/{N}/$n/g" gradQuantumProblemSet$n.tex ;

for i in 1 2 3 4 ; do 
   cp ../gradQuantumProblemSetNProblemM.tex gradQuantumProblemSet${n}Problem$i.tex
   perl -p -i -e "s/{N}/$n/g;s/{M}/$i/g;" gradQuantumProblemSet${n}Problem$i.tex
   git add gradQuantumProblemSet${n}Problem$i.tex
done

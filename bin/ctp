n=2b
~/bin/ct multiphysicsProblemSet$n multiphysicsProblemSetN tp$n
perl -p -i -e "s/{N}/$n/g" multiphysicsProblemSet$n.tex ;

for i in 1 2 ; do 
   cp ../multiphysicsProblemSetNProblemM.tex multiphysicsProblemSet${n}Problem$i.tex
   perl -p -i -e "s/{N}/$n/g;s/{M}/$i/g;" multiphysicsProblemSet${n}Problem$i.tex
   git add multiphysicsProblemSet${n}Problem$i.tex
done

foreach (@l)
{
   my $f = $_ ;
   $f =~ s,.*/,, ;
   $f =~ s,\.tex$,, ;

   print qq(perl -p -i -e 's/^\\\\begin{align\\*}/\\\\begin{equation}\\\\label{eqn:$f:n}\n\\\\begin{aligned}/;s/^\\\\end{align\\*}/\\\\end{aligned}\n\\\\end{equation}/;s/^\\\\begin{align} *\$/\\\\begin{equation}\\\\label{eqn:$f:n}\n\\\\begin{aligned}/;s/^\\\\end{align}/\\\\end{aligned}\n\\\\end{equation}/;s/^\\\\begin{align}\\\\label{(.*)} *\$/\\\\begin{equation}\\\\label{\$1}\n\\\\begin{aligned}/;' $_\n) ;
   print qq(~/bin/lgrep $_ | tee o ; . ./o\n) ;
}
